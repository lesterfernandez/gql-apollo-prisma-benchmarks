{
  "name": "fastify-benchmarks",
  "version": "1.0.0",
  "description": "Benchmarks for Fastify, a fast and low-overhead web framework.",
  "main": "index.js",
  "scripts": {
    "start": "node benchmark.js",
    "initDB": "docker-compose -f lib/docker-compose.yml up -d && yarn prisma migrate dev && yarn prisma db seed",
    "startDB": "docker-compose -f lib/docker-compose.yml up -d ",
    "compare": "node benchmark.js compare --",
    "test": "standard | snazzy",
    "standard": "standard | snazzy",
    "metrics:run": "node metrics/startup.js",
    "metrics:summary": "node metrics/process-results.js -u",
    "prepare": "husky install",
    "lint": "yarn prettier -w ."
  },
  "prisma": {
    "schema": "lib/prisma/schema.prisma",
    "seed": "node lib/prisma/seed.js"
  },
  "bin": {
    "benchmark": "./benchmark.js"
  },
  "repository": "https://github.com/fastify/benchmarks.git",
  "author": "Çağatay Çalı",
  "contributors": [
    {
      "name": "Stefan Aichholzer",
      "email": "theaichholzer@gmail.com",
      "url": "https://github.com/aichholzer"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@leizm/web": "^2.7.0",
    "@prisma/client": "^3.13.0",
    "apollo-server": "^3.6.7",
    "apollo-server-core": "^3.6.7",
    "apollo-server-express": "^3.6.7",
    "apollo-server-koa": "^3.6.7",
    "autocannon": "^7.0.1",
    "autocannon-compare": "^0.4.0",
    "benchmark": "^2.1.4",
    "chalk": "^4.1.2",
    "cli-table": "^0.3.1",
    "commander": "^9.0.0",
    "connect": "^3.6.6",
    "cors": "^2.8.5",
    "dns-prefetch-control": "^0.3.0",
    "express": "^4.16.4",
    "express-graphql": "^0.12.0",
    "fastify": "^3.0.0",
    "graphql": "^16.4.0",
    "hide-powered-by": "^1.0.0",
    "inquirer": "^8.0.0",
    "koa": "^2.7.0",
    "ora": "^5.4.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "prisma": "^3.13.0",
    "snazzy": "^9.0.0",
    "standard": "^17.0.0"
  }
}
